{
  "name": "fortune-telling-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf backend/dist && rimraf backend/tsconfig.backend.tsbuildinfo && rimraf dist && rimraf tsconfig.app.tsbuildinfo && rimraf tsconfig.node.tsbuildinfo",
    "build": "npm run build:backend && npm run build:frontend",
    "build:frontend": "tsc -b && vite build",
    "build:backend": "tsc -p backend/tsconfig.backend.json",
    "start:backend": "node backend/dist/index.js",
    "dev": "vite",
    "dev:backend": "cd backend && ts-node src/index.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "npm install --only=dev"
  },
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@react-three/drei": "^9.114.0",
    "@react-three/fiber": "^8.17.9",
    "@vitejs/plugin-react": "^4.3.2",
    "fastify": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.169.0"
  },
  "devDependencies": {
    "@types/fastify-cors": "^0.1.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.9.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  }
}
